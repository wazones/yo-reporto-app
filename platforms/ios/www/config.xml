<?xml version='1.0' encoding='utf-8'?>
<widget id="co.gov.gestiondelriesgo.yoreporto" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Yo Reporto</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@callback.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <icon src="img/icono.png" gap:role="default" />
    <content src="index.html" />
    <access origin="*" />
    <access origin="http://www.gestiondelriesgo.gov.co/" />
    <access uri="http://www.gestiondelriesgo.gov.co/" subdomains="true" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <feature name="http://api.phonegap.com/1.0/geolocation" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="websecurity" value="disable" gap:platform="blackberry10" />
</widget>
